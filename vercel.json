{
  "version": 2,
  "builds": [
    { "src": "api/*.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/predictions", "dest": "/api/predictions" },
    { "src": "/predictions.html", "dest": "/api/predictions" }
  ],
  "functions": {
    "api/predictions.ts": {
      "includeFiles": "predictions.html"
    }
  }
}
